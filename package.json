{
  "name": "pzmodhighlight",
  "displayName": "PZModHighlight",
  "description": "Project Zomboid mod highlight",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "lua",
        "aliases": [
          "PZMod",
          "lua"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lua",
        "scopeName": "source.txt",
        "path": "./syntaxes/txt.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.8.0"
  }
}
