{
	"manifestVersion": 1,
	"name": "pzmod-extention",
	"displayName": "PZMod-Extention",
	"description": "Project Zomboid Mod Extention",
	"author": "PlanC",
	"license": "MIT",
	"version": "2.0.1",
	"publisher": "PlanC",
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^1.67.0"
	},
	"repository": {
		"url": "https://github.com/iPlanC/pzmod-extention"
	},
	"icon": "icon.png",
	"contributes": {
		"snippets": [
			{
				"language": "PZMod",
				"path": "./snippets/snippets.json"
			}
		],
		"languages": [
			{
				"id": "PZMod",
				"aliases": [
					"PZMod"
				],
				"extensions": [
					".txt"
				],
				"configuration": "./language-configuration.json",
				"icon": {
					"dark": "./icon.png",
					"light": "./icon.png"
				}
			}
		],
		"grammars": [
			{
				"language": "PZMod",
				"scopeName": "source.txt",
				"path": "./syntaxes/pzmod.tmLanguage.json"
			}
		]
	}
}
